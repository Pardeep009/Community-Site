<html>
  <head>
    <% include css') -%>
    <title>Profile</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="common.css">
    <link rel="stylesheet" href="profile.css">
    <link rel="stylesheet" href="editpage.css">
  </head>
  <body>

      <% include header') -%>

    <div class="screen">
      <% include sidebar') -%>
      <div class="home-page  container-fluid">
          <div class="parent container-div">
                <div class="desktop-dp">
                  <div class="container-dp container-div">
                    <img src="dp.png" id="desktop-dp" />
                  </div>
                </div>

                <div class="form-group editbutton">
                    <a href="#" class="btn btn-primary" style="margin-right:15px; display:block;">Edit</a>
                </div>

                <div class="flex">

                    <div class="left container-div">
                      <div class="desc">
                        Email
                      </div>
                      <div class="value">
                        <%= obj.username %>
                      </div>
                    </div>

                    <div class="right container-div">
                      <div class="desc">
                        Gender
                      </div>
                      <div class="value">
                        <%= obj.gender %>
                      </div>
                    </div>

                </div>

                <div class="flex">

                    <div class="left container-div">
                      <div class="desc">
                        Name
                      </div>
                      <div class="value">
                        <%= obj.name %>
                      </div>
                    </div>

                    <div class="right container-div">
                      <div class="desc">
                        City
                      </div>
                      <div class="value">
                        <%= obj.city %>
                      </div>
                    </div>

                </div>

                <div class="flex">

                    <div class="left container-div">
                      <div class="desc">
                        DOB
                      </div>
                      <div class="value">
                        <%= obj.dob %>
                      </div>
                    </div>

                    <div class="right container-div">
                      <div class="desc">
                        Phone
                      </div>
                      <div class="value">
                        <%= obj.phone %>
                      </div>
                    </div>

                </div>

          </div>
      </div>
    </div>
  </body>
  <% include js') -%>
  <script type="text/javascript" src="profile.js"></script>
  <script type="text/javascript" src="editpage.js"></script>
</html>

